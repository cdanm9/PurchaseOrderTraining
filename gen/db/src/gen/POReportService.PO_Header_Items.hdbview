VIEW POReportService_PO_Header_Items AS SELECT
  POHEADERITEMS_0.PO_NUMBER,
  POHEADERITEMS_0.CREATIONDATE,
  POHEADERITEMS_0.DELIVERYDATE,
  POHEADERITEMS_0.STATUS,
  POHEADERITEMS_0.CREATEDBY,
  POHEADERITEMS_0.PO_ITEM_NUM,
  POHEADERITEMS_0.QUANTITY,
  POHEADERITEMS_0.AMOUNT
FROM POHEADERITEMS AS POHEADERITEMS_0